//servo motor control\par
void servocheck() \{\par
  if(page_counter != 1) \{             //when page counter not equal 1 (mode 2, 3, 4)\par
        servo2_angle = 0;\par
        if(servo2_angle == 0) \{       //servo motor(control volume) = 0, then turn 60 degree\par
          delay(100);                 //delay 0.1s\par
          servo2_angle = 60;\par
          servo2.write(servo2_angle);\par
          spray();                    //call spray function\par
          servo2.write(0);\par
        \} \par
      \}\par
      else if(page_counter == 1) \{    //when page counter equal 1 (mode 1)\par
        if(servo_angle == 0) \{        //servo motor(control spray switch) = 0, then turn 180 degree\par
          for(servo_angle = 0; servo_angle < 30; servo_angle++) \{\par
              servo.write(servo_angle);\par
            \}\par
          spray();                    //call spray function\par
        \} else \{\par
          servo_angle = 0;\par
          servo.write(servo_angle);\par
          spray();\par
        \}\par
      \}\par
\}\par
