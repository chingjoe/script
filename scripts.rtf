      if(page_counter < 4) \{      //rotary encoder turn CW the page counter ++, max of page/mode = 4\par
        page_counter++;\par
      \}else \{\par
        page_counter = 4;\par
      \}\par
    \}else\{\par
      if(page_counter > 1) \{      //rotary encoder turn CCW page counter --, min = 1\par
        page_counter--;\par
      \}else \{\par
        page_counter = 1;\par
      \}\par
    \}\par
   \}\par
   if(submenu == 1)\{             //submenu 1\par
   if(digitalRead(PinDT) != CLKNow) \{\par
      if(counter < 6) \{\par
        counter++;               //rotary encoder turn CW --> counter++\par
      \}else \{\par
        counter = 6;             //max counter = 6\par
      \}\par
    \}else\{\par
      if(counter > 1) \{          //rotary encoder turn CCW --> counter--\par
        counter--;\par
      \}else \{\par
        counter = 1;             //min counter = 1\par
      \}\par
